			tpj("input[type='radio']").click(function () {
				var payment_method_val = tpj("input[name='payment_method']:checked").val();
				if(payment_method_val) {
				  if(payment_method_val == 'bank') {
					tpj('#opt_bank').removeClass('d-none');
					tpj('#opt_cheque').addClass('d-none');
					tpj('#opt_paypal').addClass('d-none');
					tpj('#opt_venmo').addClass('d-none');
					tpj('#opt_zelle').addClass('d-none');
					tpj('#opt_amazon_gcard').addClass('d-none');
					tpj('#opt_cash').addClass('d-none');
				  } else if(payment_method_val == 'cheque') {
					tpj('#opt_bank').addClass('d-none');
					tpj('#opt_cheque').removeClass('d-none');
					tpj('#opt_paypal').addClass('d-none');
					tpj('#opt_venmo').addClass('d-none');
					tpj('#opt_zelle').addClass('d-none');
					tpj('#opt_amazon_gcard').addClass('d-none');
					tpj('#opt_cash').addClass('d-none');
				  } else if(payment_method_val == 'paypal') {
					tpj('#opt_bank').addClass('d-none');
					tpj('#opt_cheque').addClass('d-none');
					tpj('#opt_paypal').removeClass('d-none');
					tpj('#opt_venmo').addClass('d-none');
					tpj('#opt_zelle').addClass('d-none');
					tpj('#opt_amazon_gcard').addClass('d-none');
					tpj('#opt_cash').addClass('d-none');
				  } else if(payment_method_val == 'venmo') {
					tpj('#opt_bank').addClass('d-none');
					tpj('#opt_cheque').addClass('d-none');
					tpj('#opt_paypal').addClass('d-none');
					tpj('#opt_venmo').removeClass('d-none');
					tpj('#opt_zelle').addClass('d-none');
					tpj('#opt_amazon_gcard').addClass('d-none');
					tpj('#opt_cash').addClass('d-none');
				  } else if(payment_method_val == 'zelle') {
					tpj('#opt_bank').addClass('d-none');
					tpj('#opt_cheque').addClass('d-none');
					tpj('#opt_paypal').addClass('d-none');
					tpj('#opt_venmo').addClass('d-none');
					tpj('#opt_zelle').removeClass('d-none');
					tpj('#opt_amazon_gcard').addClass('d-none');
					tpj('#opt_cash').addClass('d-none');
				  } else if(payment_method_val == 'amazon_gcard') {
					tpj('#opt_bank').addClass('d-none');
					tpj('#opt_cheque').addClass('d-none');
					tpj('#opt_paypal').addClass('d-none');
					tpj('#opt_venmo').addClass('d-none');
					tpj('#opt_zelle').addClass('d-none');
					tpj('#opt_amazon_gcard').removeClass('d-none');
					tpj('#opt_cash').addClass('d-none');
				  } else if(payment_method_val == 'cash') {
					tpj('#opt_bank').addClass('d-none');
					tpj('#opt_cheque').addClass('d-none');
					tpj('#opt_paypal').addClass('d-none');
					tpj('#opt_venmo').addClass('d-none');
					tpj('#opt_zelle').addClass('d-none');
					tpj('#opt_cash').removeClass('d-none');
					tpj('#opt_amazon_gcard').addClass('d-none');
				  }
				}
			});
			
			function check_step3_validations() {
				var first_name = tpj("#first_name").val().trim();
				var last_name = tpj("#last_name").val().trim();
				var phone = tpj("#phone").val().trim();
				var email = tpj("#email").val().trim();
	
				var mailformat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
	
				if(first_name == "") {
					alert("Please enter first name");
					return false;
				} else if(last_name == "") {
					alert("Please enter last name");
					return false;
				} else if(phone == "") {
					alert("Please enter phone number");
					return false;
				} 
				
				var telInput = tpj("#phone");
				tpj("#phone_c_code").val(telInput.intlTelInput("getSelectedCountryData").dialCode);
				if(!telInput.intlTelInput("isValidNumber")) {
					alert('Please enter valid phone');
					return false;
				}
				
				if(email == "") {
					alert("Please enter email address");
					return false;
				} else if(email!="" && !email.match(mailformat)) {
					alert("You have entered an invalid email address!");
					return false;
				}
			}
	
			function check_step4_validations() {
				<?php
				if($choosed_payment_option['bank']=="bank") { ?>
				if(document.getElementById("payment_method_bank").checked==true) {
					if(document.getElementById("act_name").value.trim()=="") {
						alert('Please enter account holder name');
						return false;
					} else if(document.getElementById("act_number").value.trim()=="") {
						alert('Please enter account number');
						return false;
					} else if(document.getElementById("act_short_code").value.trim()=="") {
						alert('Please enter short code');
						return false;
					}
				}
				<?php
				}
				if($choosed_payment_option['paypal']=="paypal") { ?>
				if(document.getElementById("payment_method_paypal").checked==true) {
					if(document.getElementById("paypal_address").value.trim()=="") {
						alert('Please enter paypal address');
						return false;
					}
				}
				<?php
				}
				if($choosed_payment_option['venmo']=="venmo") { ?>
				if(document.getElementById("payment_method_venmo").checked==true) {
					if(document.getElementById("venmo_email_address").value.trim()=="") {
						alert('Please enter venmo email address');
						return false;
					}
				}
				<?php
				}
				if($choosed_payment_option['zelle']=="zelle") { ?>
				if(document.getElementById("payment_method_zelle").checked==true) {
					if(document.getElementById("zelle_email_address").value.trim()=="") {
						alert('Please enter zelle email address');
						return false;
					}
				}
				<?php
				}
				if($choosed_payment_option['amazon_gcard']=="amazon_gcard") { ?>
				if(document.getElementById("payment_method_amazon_gcard").checked==true) {
					if(document.getElementById("amazon_gcard_email_address").value.trim()=="") {
						alert('Please enter amazon GCard email address');
						return false;
					}
				}
				<?php
				}
				if($choosed_payment_option['cash']=="cash") { ?>
				if(document.getElementById("payment_method_cash").checked==true) {
					if(document.getElementById("cash_name").value.trim()=="") {
						alert('Please enter cash name');
						return false;
					} else if(document.getElementById("cash_phone").value.trim()=="") {
						alert('Please enter cash phone');
						return false;
					}

					var telInput3 = $("#cash_phone");
					$("#f_cash_phone").val(telInput3.intlTelInput("getNumber"));
					if(!telInput3.intlTelInput("isValidNumber")) {
						alert('Please enter valid cash phone');
						return false;
					}
				}
				<?php
				} ?>
			}
		});
	})(jQuery);
