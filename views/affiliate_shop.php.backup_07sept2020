			tpj(".step5next").on("click", function() {
				var ok = check_step3_validations();
				if(ok == false) {
					return false;
				}
				
				var ok2 = check_step4_validations();
				if(ok2 == false) {
					return false;
				}
	
				var billing_first_name = tpj("#billing_first_name").val().trim();
				var billing_last_name = tpj("#billing_last_name").val().trim();
				var billing_phone = tpj("#billing_phone").val().trim();
				var billing_address = tpj("#billing_address").val().trim();
				var billing_city = tpj("#billing_city").val().trim();
				var billing_state = tpj("#billing_state").val().trim();
				var billing_postcode = tpj("#billing_postcode").val().trim();
				var billing_company_name = tpj("#billing_company_name").val().trim();
				if(billing_first_name == "") {
					alert("Please enter shipping first name");
					return false;
				} else if(billing_last_name == "") {
					alert("Please enter shipping last name");
					return false;
				} else if(billing_address == "") {
					alert("Please enter shipping address");
					return false;
				} else if(billing_city == "") {
					alert("Please enter shipping city");
					return false;
				} else if(billing_state == "") {
					alert("Please enter shipping state");
					return false;
				} else if(billing_postcode == "") {
					alert("Please enter shipping post code");
					return false;
				} else if(billing_phone == "") {
					alert("Please enter shipping phone");
					return false;
				}
				
				var telInput2 = tpj("#billing_phone");
				tpj("#billing_phone_c_code").val(telInput2.intlTelInput("getSelectedCountryData").dialCode);
				if(!telInput2.intlTelInput("isValidNumber")) {
					alert('Please enter valid shipping phone');
					return false;
				}
		
				if(tpj("#terms_and_conditions").prop("checked") == false) {
					alert("You must agree to terms & conditions.");
					return false;
				}
	
				tpj("#apr_form").submit();
				return true;
			});
			
			tpj("#get_imei_info").click(function(e) {
				var imei_number = tpj("#imei_number").val().trim();
				if(imei_number == "") {
					tpj("#imei_number").focus();
					return false;
				}
				post_data = "imei_number="+imei_number+"&mode=imei&token=<?=get_unique_id_on_load()?>";
				tpj(document).ready(function(tpj){
					tpj.ajax({
						type: "POST",
						url:"<?=SITE_URL?>ajax/affiliate/get_model_list.php",
						data:post_data,
						success:function(data) {
							if(data!="") {
								tpj("#model_list").html(data);
								tpj("#model_fields_list").html("");
								tpj("#collapseTwo").collapse('show');
								tpj("#collapseTwo").addClass('show');
								tpj("#collapse_two_pos").show();
								
								tpj("#collapseOne").collapse('hide');
								tpj("#collapseOne").removeClass('show');
								tpj(".first-step-arrow-up").hide();
								tpj(".first-step-arrow-down").show();
								
								setTimeout(function() {
									tpj.scrollTo(tpj('#collapse_two_pos'), 500);
								}, 500);
							} else {
								tpj("#model_list").html("");
								tpj("#model_fields_list").html("");
							}
						}
					});
				});
			});
			
